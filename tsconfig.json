{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@containers/*": ["src/containers/*"],
      "@store/*": ["src/store/*"],
      "@routes/*": ["src/routes/*"],
      "@assets/*":["src/assets/*"],
      "@config/*":["src/config/*"],
    }
  }
}
